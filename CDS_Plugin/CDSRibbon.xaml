<?xml version="1.0" encoding="utf-8" ?>
<!-- BEGIN STANDARD SECTION DO NOT EDIT 
    except for ensuring x:Uid="CustomRibbonTab" has a unique ID
    -->
<RibbonControl
    x:Uid="CustomRibbon" xmlns="clr-namespace:Autodesk.Windows;assembly=AdWindows"
    xmlns:wpf="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:adwi="clr-namespace:Autodesk.Internal.Windows;assembly=AdWindows"
    xmlns:system="clr-namespace:System;assembly=mscorlib"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Autodesk.Navisworks.Gui.Roamer.AIRLook;assembly=navisworks.gui.roamer">
    <!-- END STANDARD SECTION DO NOT EDIT -->
    
    <!--
    Прописывание внешнего вида ссылки на плагин в верхней панели Navis
    -->
    <RibbonTab Id="CDS_Plugin" KeyTip="T1">
        <!--
        Прописывание внешнего вида группы кнопок и самих кнопок внутри ссылки в верхней панели Navis
        -->
        <RibbonPanel x:Uid="RibbonPane1">
            <RibbonPanelSource x:Uid="RibbonPanelSource_RibbonPanel_1" KeyTip="C1" Title="Colorize">
                <!--
                Кнопка для определения роли
                -->
                <local:NWRibbonButton x:Uid="Button_1.0" Id="BtRole"
                            Size="Large"
                            KeyTip="B4"
                            ShowText="True"
                            Text="Роль"
                            Orientation="Vertical"/>
                 <!--
                 Выпадающий список кнопок для ИТР
                 -->        
                <local:NWRibbonSplitButton x:Uid="Button_1.1" Id="ITR"
                                 IsSynchronizedWithCurrentItem="False"
                                 IsSplit="False"
                                 ShowText="True"
                                 Text="ИТР"
                                 Orientation="Vertical"
                                 Size="Large"
                                 KeyTip="S1">

                    <local:NWRibbonButton x:Uid="Button_1.1.1" Id="ITRBt1"
                                Size="Large"
                                KeyTip="B1"
                                ShowText="True"
                                Text="Выполнено"
                                Orientation="Vertical"
                              />
                    <local:NWRibbonButton x:Uid="Button_1.1.2" Id="ITRBt2"
                                 Size="Large"
                                 KeyTip="B2"
                                 ShowText="True"
                                 Text="Устранено"
                                 Orientation="Vertical" 
                                    />
                </local:NWRibbonSplitButton>
                
                <!--
                 Выпадающий список кнопок для УСК
                 -->
                
                <local:NWRibbonSplitButton x:Uid="Button_1.2" Id="USK"
                     IsSynchronizedWithCurrentItem="False"
                     IsSplit="False"
                     ShowText="True"
                     Text="УСК"
                     Orientation="Vertical"
                     Size="Large"
                     KeyTip="S1">

                        <local:NWRibbonButton x:Uid="Button_1.2.1" Id="USKBt1"
                    Size="Large"
                    KeyTip="B1"
                    ShowText="True"
                    Text="Принято"
                    Orientation="Vertical"
                  />
                        <local:NWRibbonButton x:Uid="Button_1.2.2" Id="USKBt2"
                     Size="Large"
                     KeyTip="B2"
                     ShowText="True"
                     Text="Замечание"
                     Orientation="Vertical" 
                        />

                    </local:NWRibbonSplitButton>


                <!--<local:NWRibbonSplitButton x:Uid="Button_1.0" Id="AddClChoice"
                                                IsSynchronizedWithCurrentItem="False"
                                                IsSplit="False"
                                                ShowText="True"
                                                Text="Статус"
                                                Orientation="Vertical"
                                                Size="Large"
                                                KeyTip="S1">


                    <local:NWRibbonButton x:Uid="Button_1.1" Id="AddClBt1"
                            Size="Large"
                            KeyTip="B1"
                            ShowText="True"
                            Text="Выполнено"
                            Orientation="Vertical"
                          />

                    <local:NWRibbonButton x:Uid="Button_1.2" Id="AddClBt2"
                        Size="Large"
                        KeyTip="B2"
                        ShowText="True"
                        Text="Замечания"
                        Orientation="Vertical" 
                           />

                    <local:NWRibbonButton x:Uid="Button_1.3" Id="AddClBt3"
                            Size="Large"
                            KeyTip="B3"
                            ShowText="True"
                            Text="Принято"
                            Orientation="Vertical"
                            />
                </local:NWRibbonSplitButton>-->

            
                <local:NWRibbonButton x:Uid="Button_1.4" Id="RstClEl"
                            Size="Large"
                            KeyTip="B4"
                            ShowText="True"
                            Text="Сброс"
                            Orientation="Vertical"/>
                
                <local:NWRibbonButton x:Uid="Button_1.5" Id="RstClAll"
                            Size="Large"
                            KeyTip="B5"
                            ShowText="True"
                            Text="Сброс"
                            Orientation="Vertical"/>
                
                

            </RibbonPanelSource>
  
        </RibbonPanel>


        <RibbonPanel x:Uid="RibbonPane2">

            <RibbonPanelSource x:Uid="RibbonPanelSource_RibbonPanel_2" KeyTip="T2" Title="Add parameters">
                <!--
                Прописывание внешнего вида кнопок внутри группы кнопок
                -->
                <local:NWRibbonSplitButton x:Uid="Button_2.0" Id="Add_p"
                                                IsSynchronizedWithCurrentItem="False"
                                                IsSplit="False"
                                                ShowText="True"
                                                Text="Парам."
                                                Orientation="Vertical"
                                                Size="Large"
                                                KeyTip="S2">


                    <local:NWRibbonButton x:Uid="Button_2.1" Id="Add_all_p_xlsx"
                            Size="Large"
                            KeyTip="P7"
                            ShowText="True"
                            Text="Модель"
                            Orientation="Vertical"/>

                    <local:NWRibbonButton x:Uid="Button_2.2" Id="Add_el_p_xlsx"
                            Size="Large"
                            KeyTip="P8"
                            ShowText="True"
                            Text="Элемент"
                            Orientation="Vertical"/>

                </local:NWRibbonSplitButton>

                <local:NWRibbonButton x:Uid="Button_2.3" Id="SetSettings"
                        Size="Large"
                        KeyTip="P4"
                        ShowText="True"
                        Text="Наборы"
                        Orientation="Vertical"/>


                <!--<local:NWRibbonButton x:Uid="Button_2.4" Id="TestTab"
                        Size="Large"
                        KeyTip="P5"
                        ShowText="True"
                        Text="Тест"
                        Orientation="Vertical"/>-->

            </RibbonPanelSource>

        </RibbonPanel>

        
        <RibbonPanel x:Uid="RibbonPane3">
            <RibbonPanelSource x:Uid="RibbonPanelSource_RibbonPanel_3" KeyTip="T3" Title="Export|Import">
                
                
                <local:NWRibbonButton x:Uid="Button_3.1" Id="Export"
                            Size="Large"
                            KeyTip="B6"
                            ShowText="True"
                            Text="Экспорт"
                            Orientation="Vertical"/>

                <local:NWRibbonButton x:Uid="Button_3.1" Id="Test"
                           Size="Large"
                           KeyTip="B9"
                           ShowText="True"
                           Text="Тест"
                           Orientation="Vertical"/>         

            </RibbonPanelSource>
 
        </RibbonPanel>
        
        <RibbonPanel x:Uid="RibbonPane4">
            <RibbonPanelSource x:Uid="RibbonPanelSource_RibbonPanel_3" KeyTip="T4" Title="Quanti">


                <local:NWRibbonButton x:Uid="Button_4.1" Id="AutoQuanti"
                           Size="Large"
                           KeyTip="B10"
                           ShowText="True"
                           Text="Auto"
                           Orientation="Vertical"/>

            </RibbonPanelSource>

        </RibbonPanel>

    </RibbonTab>

</RibbonControl>